  $('a').each(function(){
    let target = $(this).attr('href'); // e.g. "#1"
    if (target.startsWith('#') && target.length > 1) {
      let id = target.substring(1); // remove #
      if ($('#' + id).length === 0) {
        $('body').append('<div id="'+ id +'" class="anchor-target"></div>');
      }
    }
  });






  (function() {
    // Function to apply aria-label to the specific CTA anchors
    function applyAriaToCTAs() {
      const selector = '.hs-cta-wrapper .hs-cta-node a';
      document.querySelectorAll(selector).forEach(btn => {
        if (btn.getAttribute('target') === '_blank') {
          btn.setAttribute('aria-label', 'Learn More (opens in a new tab)');
        } else {
          btn.setAttribute('aria-label', 'Learn More');
        }
      });
    }

    // Run immediately in case CTAs already exist
    applyAriaToCTAs();

    // Run again after short delay to catch delayed HubSpot CTA loads
    setTimeout(applyAriaToCTAs, 200);

    // Also reapply when HubSpot form events fire (in case CTA loads after form)
    window.addEventListener('message', event => {
      if (event && event.data && event.data.type === 'hsFormCallback' && event.data.eventName === 'onFormReady') {
        applyAriaToCTAs();
        setTimeout(applyAriaToCTAs, 200);
      }
    });

    // Observe DOM for newly inserted CTAs
    const observer = new MutationObserver(() => {
      applyAriaToCTAs();
    });

    // Start watching for CTA inserts
    document.addEventListener('DOMContentLoaded', () => {
      observer.observe(document.body, { childList: true, subtree: true });
    });
  })();

</script>

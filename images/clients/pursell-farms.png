    // Object to keep track of how many times a link text has been used
    var linkCount = {};

    jQuery('a').each(function() {
      var $link = jQuery(this);

      // Skip if aria-label already exists
      if ($link.attr('aria-label')) return;

      // Get link text
      var linkText = $link.text().trim();
      if (!linkText) return;

      // Initialize count if not exists
      if (!linkCount[linkText]) {
        linkCount[linkText] = 1;
      } else {
        linkCount[linkText]++;
      }

      // Construct aria-label
      var ariaLabel = linkText;

      // If multiple links with same text, append number
      if (linkCount[linkText] > 1) {
        ariaLabel += " " + linkCount[linkText];
      }

      ariaLabel += " Link";

      // Add note for new-tab links
      if ($link.attr('target') === '_blank') {
        ariaLabel += " - (Open in new window)";
      }

      // Apply aria-label
      $link.attr('aria-label', ariaLabel);
